# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=rental_management
DB_USER=root
DB_PASSWORD=12346

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
